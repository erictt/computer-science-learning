{
    "title": "Computer Science Learning",
    "description": " notes, exercise and some other stuff.",
    "author": "Eric Yang",
    "plugins": [
        "katex@git+https://github.com/erictt/plugin-katex.git#2.1.0",
        "simple-page-toc@git+https://github.com/erictt/gitbook-plugin-simple-page-toc.git",
        "github",
        "anchors",
        "splitter",
        "highlight",
        "anchor-navigation-ex",
        "expandable-chapters",
        "sitemap-general"
    ],
    "pluginsConfig": {
        "simple-page-toc": {
            "maxDepth": 3,
            "skipFirstH1": true
        },
        "sitemap-general": {
            "prefix": "https://cs.ericyy.me/"
        },
        "github": {
            "url": "https://github.com/erictt/computer-science-learning"
        },
        "anchor-navigation-ex": {
            "multipleH1": false
        },
		"analytics": {
			"google": "UA-56541906-1"
		}
    },
    "gitbook": ">=3.0.0"
}
